{
    "seed": 42,
    "model_dir": "klue/roberta-small",
    "data": {
        "data_dir": "data/train.csv",
        "aug_data_dir": "",
        "split_ratio": 0.1,
        "n_splits": 5,
        "add_ent_token": true
    },
    "training_arguments": {
        "hyperparameter": {
            "batch_size": 32,
            "gradient_accumulation_steps": 4,
            "learning_rate": 5e-5,
            "weight_decay": 0.3,
            "epochs": 10
        },
        "output_dir": "results/",
        "save_dir": "best_model/",
        "logging_dir": "logs/",
        "logging_step": 200,
        "save_total_limit": 2,
        "evaluation_strategy": "steps",
        "eval_steps": 200,
        "save_steps": 200,
        "load_best_model_at_end": true,
        "metric_for_best_model": "micro f1 score",
        "fp16": true,
        "fp16_opt_level": "O1"
    },
    "wandb": {
        "project": "klue",
        "entity": "chungye-mountain-sherpa",
        "name": "base",
        "group": "klue/roberta-small"
    }
}